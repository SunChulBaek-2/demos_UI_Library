<?xml version="1.0" encoding="utf-8"?>
<resources>
	<style name="AppTheme" parent="RedColorAppTheme"/>

	<style name="BaseAppTheme" parent="android:Theme.Material.Light">
		<item name="android:windowActionBar">false</item>
		<item name="android:windowNoTitle">true</item>

		<!-- enable window content transitions -->
		<item name="android:windowContentTransitions">true</item>

		<!-- specify enter and exit transitions -->
		<item name="android:windowEnterTransition">@android:transition/explode</item>
		<item name="android:windowExitTransition">@android:transition/explode</item>
	</style>

	<style name="RedColorAppTheme" parent="BaseAppTheme">
		<item name="android:colorPrimary">@color/red500</item>
		<item name="android:textColorPrimary">@color/lightTextPrimary</item>
		<item name="android:colorPrimaryDark">@color/red700</item>
		<item name="android:navigationBarColor">@android:color/black</item>
	</style>

