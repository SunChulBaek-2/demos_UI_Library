<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
			  xmlns:tools="http://schemas.android.com/tools"
			  android:layout_width="match_parent"
			  android:layout_height="match_parent"
			  android:background="#eeeeee"
			  android:orientation="vertical"
			  tools:context=".MainActivity">

	<Button
		android:id="@+id/activity_main_separate_rfab_sample_btn"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:text="separate rfab sample"
		android:textAllCaps="false"
		/>

	<Button
		android:id="@+id/activity_main_label_list_sample_btn"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:text="label list sample"
		android:textAllCaps="false"
		/>

	<Button
		android:id="@+id/activity_main_rfab_group_btn"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:text="rfab group sample"
		android:textAllCaps="false"
		/>


</LinearLayout>
